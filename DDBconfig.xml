    параметры без value (установленный параметр) - выводятся в текстовом виде в окне параметров и только там.
    real - измеренный параметр
    Параметры с value "участвуют" в hide controls.
     -->
    <ParamControls>
      <Param unit="Hz" min="0101" max="0102" value="0100" divider="10">Frequency</Param>
      <Param unit="ms" min="0201" max="0202" value="0200" divider="10">Duration</Param>
      <Param unit="A" min="0301" max="0302" value="0300" real="0307" divider="10">Current</Param>
      <Param unit=" " real="0701" divider="1">Serial Number</Param>
      <Param unit="V" real="0407" divider="10">Voltage</Param>
	  <Param unit="(deg)" isTemperature="1" real="0AF4" divider="10">Driver Temperature</Param>
      <Param unit="(deg)" isTemperature="1" real="0AE4" divider="10">NTC Temperature</Param>
    </ParamControls>
    <!--  список checkbox (Галочек) Имеют code - номер команды два значения при нажатии и отщёлкивании галочки.
    Могут также программироваться по ID лично в проге, тогда остальные поля оставлять пустыми,
    хотя можно попробовтаь комбинировать, но насколько синхронно это будет работать есть сомнения -->
    <BinaryOptions>
      <BinOption code="0700" onCommand="0020" offCommand="0040" mask="0004" id="1">Internal Current Set</BinOption>
      <BinOption code="0700" onCommand="0400" offCommand="0200" mask="0010" id="2">Internal Enable</BinOption>
      <BinOption code="0700" onCommand="4000" offCommand="8000" mask="0040" id="3">Ignore LD Overheat Block</BinOption>
      <BinOption code="0700" onCommand="2000" offCommand="1000" mask="0080" id="4">Ignore IntLock</BinOption>
    </BinaryOptions>
    <!-- кнопочки доступные в программе. Аналогично чекбоксам, только тут чисто по протоколу -->
    <Buttons>
      <Button name="laser" mask="02" code="0700" onCommand="0008" offCommand="0010"/>
    </Buttons>
    <Leds>
      <Led label="Laser">
        <LedMask Command="0700" mask="0002" maskColor="#00ff00" defaultColor="#171717"/>
      </Led>
      <Led label="IntLock">
        <LedMask Command="0800" mask="0002" maskColor="#ffff00">Interlock</LedMask>
        <LedMask Command="0800" mask="0020" maskColor="#ff0000">LD Overheat</LedMask>
      </Led>
	  <Led label="Error">
        <LedMask Command="0800" mask="0010" maskColor="#ffff00">Overheat warning</LedMask>
        <LedMask Command="0800" mask="0008" maskColor="#ff0000">Over current or Overheat</LedMask>
      </Led>
    </Leds>
  </Device>
</Config>
